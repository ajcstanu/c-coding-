//odd number 
#include<stdio.h>
int main() {
    int i, n;
    printf("Enter the value: ");
    scanf("%d", &n);
    for (i = 1; i <= n * 2 - 1; i += 2) {
        printf("%d ", i); // Add a space after printing each number
    }
    printf("\n"); // Add a newline after printing all numbers
    return 0;
}
